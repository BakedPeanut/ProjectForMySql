<%- include('../sections/header.ejs') %>
<%- include('../sections/menu.ejs') %>
<table class="styled-table">
    <thead>
        <tr>
            <th>Avatar</th>
            <th>Name</th>
            <th>ID</th>
            <th>Total revenue</th>
            <th>Total orders</th>
        </tr>
    </thead>
    <tbody>
        <% usr.forEach(function(ur){ %>
        <tr class="active-row">
            <td>
                <img src="<%= ur.Avatar %> " class="img_display" alt="" />
            </td>
            <td><%= ur.UserID %></td>
            <td><%= ur.FullName %></td>
            <td><%= ur.Total_revenue %></td>
            <td><%= ur.Total_orders %></td>
        </tr>
        <% }); %>
    </tbody>
</table>